   "source": [
    "d = int(input('enter a number again: '))\n",
    "\n",
    "def _fib(a,b,last):\n",
    "    if last == 0:\n",
    "        return b\n",
    "    if last:\n",
    "        return _fib(b,a+b,last-1)\n",
    "\n",
    "def fib(d):\n",
    "    return _fib(0,1,d-1)\n",
    "\n",
    "print(fib(d))"
   ]
  }
